#include<iostream>
using namespace std;

int main(){
    int n=2;
    int arr[n];
    cout<<"without swaping a and b :"<<endl;
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int temp=arr[1];
    arr[1]=arr[0];
    arr[0]=temp;
    cout<<"swaping number of array:"<<endl;
    cout<<arr[0]<<endl;
    cout<<arr[1];
    return 0;
}
